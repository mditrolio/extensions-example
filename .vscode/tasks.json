    },
    {
      "type": "npm",
      "script": "build",
      "problemMatcher": [
        "$tsc"
      ]
    },
    {
      "type": "npm",
      "script": "lint",
      "problemMatcher": [
        "$tslint5"
      ]
    }
  ]
}
